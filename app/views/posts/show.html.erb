<div class="detail">
  <%= image_tag @post.image.url, :size =>'480x480' %>
  <%= simple_format(@post.text) %>
  <div class="like_button">
    <%= render partial: 'likes/like', locals: {post: @post} %>
  </div>
  <div class="like_count">
    <%= @post.likes.count %>
  </div>
  <div class="liked_user">
    <li>イイネしたユーザー</li>
    <% @post.liked_users.each do |user| %>
      <li><%= user.nickname %></li>
    <% end %>
  </div>
</div>